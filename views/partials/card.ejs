<div class="card">
  <img src="<%= project.image %>" alt="<%= project.title %>" class="card-img" />
  <div class="card-body">
    <h3><%= project.title %></h3>
    <p><%= project.description %></p>
    <% if (project.url) { %>
      <a class="card-link" href="/<%= project.url %>">View Project</a>
    <% } %>
  </div>
</div>