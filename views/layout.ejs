<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Fira+Sans:wght@300;400;500&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Fira+Code:wght@300&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:wght@300;400;500;700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Silkscreen:wght@400;700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap" rel="stylesheet">


    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

    <link rel="stylesheet" href="layout.css">
    <link rel="stylesheet" href="/page-css/effects.css">
    <title>MaxColeWebsite</title>
</head>

<!-- Replace code background effects here -->
<body>
    <header id="main-header">
        <nav>
            <ul>
                <li><a href="./">MAX COLE</a></li>
                <li><a href="./about">ABOUT</a></li>

                <li class="dropdown">
                    <a href="./projects" class="dropbtn">PROJECTS</a>
                    <div id="projects-content" class="dropdown-content">

                        <p class="dropdown-label">Major Projects</p>
                        <% projects.forEach(project => { %>
                            <% if (project.tag === 'major') { %>
                                <a href="./<%= project.url %>" class="drop-button"><%= project.pageTitle %></a>
                            <% } %>
                        <% }); %>

                        <p class="dropdown-label">Minor Projects</p>
                        <% projects.forEach(project => { %>
                            <% if (project.tag !== 'major') { %>
                                <a href="./<%= project.url %>" class="drop-button"><%= project.pageTitle %></a>
                            <% } %>
                        <% }); %>
                    </div>
                </li>

                <li><a href="./artpage">MX.CO ART</a></li>
                <li><a href="./contact">CONTACT</a></li>
            </ul>
        </nav>
    </header>

    <!-- Main content will go here -->
    <main>
        <%- body %>
    </main>


    <div id="social-media-tab">
        <button id="tab-button">
            <img src="symbols/expand_more_FILL0_wght400_GRAD0_opsz24.svg" id="tab-symbol">
        </button>
        <div id="social-media-icons" class="hidden">
            <!-- Add your social media icons here -->
            <a href="mailto:max.cole4444@gmail.com" target="_blank"><img src="symbols/icons8-connect.svg" alt="Email Icon" style="vertical-align: middle;"></a>
            <a href="https://github.com/EatYoCharms9" target="_blank"><img src="symbols/icons8-github.svg" alt="GitHub Icon" style="vertical-align: middle;"></a>
            <a href="https://linkedin.com/in/myusername" target="_blank"><img src="symbols/icons8-linkedin.svg" alt="LinkedIn Icon" style="vertical-align: middle;"></a>
            <a href="https://www.instagram.com/mx.co.art/" target="_blank"><img src="symbols/icons8-instagram.svg" alt="Instagram Icon" style="vertical-align: middle;"></a>

                <!-- Add more icons as needed -->
        </div>
    </div>

</body>

<script src="JS-files/typingEffect.js"></script>
<script src="JS-files/buttons.js"></script>

</html>